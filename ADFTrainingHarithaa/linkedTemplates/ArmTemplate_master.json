						"value": "[parameters('Onprem_connectionString')]"
					},
					"dataflowLS_connectionString": {
						"value": "[parameters('dataflowLS_connectionString')]"
					},
					"AzureFunc_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('AzureFunc_properties_typeProperties_functionAppUrl')]"
					},
					"AzureFunctionLS_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('AzureFunctionLS_properties_typeProperties_functionAppUrl')]"
